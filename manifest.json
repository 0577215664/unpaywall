{
  "name": "GetPDF",
  "version": "0.0.1",
  "version_name": "GetPDF 0.0.1",
  "manifest_version": 2,
  "description": "Get a link to a free PDF when you view scholarly articles.",
  "icons": {
    "16": "img/favicon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "getPDF.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "geolocation"
  ]
}